{
  "name": "4u-ui",
  "keywords": [
    "react",
    "components",
    "ui"
  ],
  "description": "Easy to use components & page templates for ReactJS",
  "author": "AustinBearden",
  "version": "0.1.0",
  "private": false,
  "module": "dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AustinBearden/4u-ui.git"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "yarn compile-sass & yarn watch & react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/ --out-dir dist --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "node-sass -o src/styles src/styles/scss -w",
    "compile-sass": "node-sass -o src/styles src/styles/scss"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0"
  }
}